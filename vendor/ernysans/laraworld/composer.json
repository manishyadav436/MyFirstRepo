{
  "name": "ernysans/laraworld",
  "description": "Countries, Languages and Time Zones Package for Laravel 5.*",
  "keywords": ["laravel", "lumen", "countries", "country", "languages", "timezone", "time", "json", "migrations", "seed", "php"],
  "homepage" : "https://github.com/ernysans/laraworld",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Erny Sans",
      "email": "c@erny.co",
      "homepage": "http://erny.co",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "5.1.*",
    "laravel/laravel": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "ErnySans\\Laraworld\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": ["tests/AppTestCase.php"]
  },
  "extra": {
    "laravel": {
      "providers": [
        "ErnySans\\Laraworld\\LaraworldServiceProvider"
      ],
      "aliases": {
        "Countries": "ErnySans\\Laraworld\\Facades\\Countries",
        "TimeZones": "ErnySans\\Laraworld\\Facades\\TimeZones",
        "Languages": "ErnySans\\Laraworld\\Facades\\Languages"
      }
    }
  }
}
